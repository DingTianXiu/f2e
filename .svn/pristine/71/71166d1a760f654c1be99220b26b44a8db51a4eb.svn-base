<!-- 高级搜索 -->
<div class="high_search" id="highSearch" style="height:300px;">
  <div class="clear">
    <p class="fl p1">ID<input type="text" name="id"/></p>
    <p class="fl p2">控件名称<input type="text" name="name"/></p>
  </div>
  <div class="clear">
    <p class="fl p1">
      控件分类
      <!-- <select name="widgetCatName" id="widgetCatName">
            <option value="">请选择</option>
        </select> -->
        <input type="text" name="widgetCatName"/>
    </p>
    <p class="fl p2">最近修改人<input type="text" name="modifiedLoginName"/></p>
  </div>

  <div class="clear date2">
    <p class="fl p4" style="right:315px;">修改时间<input style="text-indent:25px;" type="text" name="startModifyTime" id="modifyStart" onfocus="WdatePicker({dateFmt:'yyyy-MM-dd HH:mm:ss',maxDate:'#F{$dp.$D(\'modifyEnd\')||\'2030-10-01\'}'})"/>至<input style="text-indent:25px;" type="text" id="modifyEnd" name="endModifyTime" onfocus="WdatePicker({dateFmt:'yyyy-MM-dd HH:mm:ss',minDate:'#F{$dp.$D(\'modifyStart\')}',maxDate:'2020-10-01'})"/><img src="./public/images/date.png" class="calender1" alt=""/><img src="./public/images/date.png" class="calender2" alt=""/></p>
  </div>
  <hr/>
  <div class="submit fr">
    <a href="javascript:;" class="a1" ng-click="advancedSearchConfirm()">确定</a>
    <a href="javascript:;" class="a2" ng-click="closeAdvancedSearch()">取消</a>
  </div>
</div>
