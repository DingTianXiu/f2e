/** 公用字体 **/
@font-face {
  font-family: 'iconfont';
  src: url('//at.alicdn.com/t/font_1472363451_049837.eot'); /* IE9*/
  src: url('//at.alicdn.com/t/font_1472363451_049837.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('//at.alicdn.com/t/font_1472363451_049837.woff') format('woff'), /* chrome、firefox */
  url('//at.alicdn.com/t/font_1472363451_049837.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
  url('//at.alicdn.com/t/font_1472363451_049837.svg#iconfont') format('svg'); /* iOS 4.1- */
}
                    
/** 公用字体 **/
.header {
  width: 100%;
  height: 60px;
  line-height: 60px;
  background: #304274;
  position: fixed;
  z-index: 10;
}
.header .logo {
  width: 160px;
  text-align: center;
  background: #1a2e66;
  color: #ffffff;
  font-size: 20px;
  font-family: arial;
}
.header .logo img {
  vertical-align: middle;
  margin-right: 8px;
}
.top_nav {}
.top_nav li {
  border-right: 1px solid #233360;
  background: #304274;
  width: 115px;
  text-align: center;
  color: #ffffff;
  font-size: 16px;
  position: relative;
}
.top_nav .li2_con {
  display: none;
  border: 1px solid #e1e6eb;
  border-bottom: none;
  background: #ffffff;
  z-index: 999;
  position: absolute;
  left: 0px;
  top: 60px;
  width: 113px;
}
.top_nav .li2_con p {
  border-bottom: 1px solid #e1e6eb;
  font-size: 14px;
  height: 40px;
  line-height: 40px;
  text-align: center;
}
.top_nav .li2_con p a {
  color: #666666;
}
.top_nav .li2_con p.focus {
  background: #ededed;
}
.top_nav li a {
  color: #ffffff;
}
.top_nav li img {
  margin-left: 8px;
  vertical-align: middle;
}
.btns {
  margin-right: 22px;
}
.btns a {
  margin-right: 42px;
  position: relative;
  top: 6px;
}
.info {
  position: absolute;
  z-index: 1000 !important;
  width: 337px; height: 158px; right: 20px; top: 25px; background: url(../images/login_info_bg.png) no-repeat;
  font-family: arial;
}
.showBox{
  height: 158px;
  display: none;
}
.personalDisplay:hover .showBox{
  display: block;
}
.info .imgContainer{
  float: left;
  margin-top: 30px;
  margin-left: 20px;
  width: 98px;
  height: 98px;
  -webkit-border-radius: 98px;
  -moz-border-radius: 98px;
  border-radius: 98px;
  overflow: hidden;
  text-align: center;
}
.info img{
  height: 98px;
  vertical-align: middle;
}
.info .personalMsg{
  float: left;
  margin-top: 45px;
}
.main_top .info li{
  display: inherit;
}
.main_top .info li a{
  display: inline-block;
  float: left;
  width: 78px;
  height: 30px;
  margin-right: 10px;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  border-radius: 4px;
  border: 1px solid #cccccc;
  background-color: #fdfdfd;
  font-size: 13px;
  color: #444444;
  line-height: 30px;
  text-decoration: none;
  text-align: center;
}
.login_out {
  display: none;
  box-shadow: 1px 1px 1px 1px #cccccc;
  position: absolute;
  top: 59px;
  left: 0px;
  width: 220px;
  height: 40px;
  line-height: 40px;
  text-align: center;
  color: #666666;
  border-top: 1px solid #666666;
  z-index: 999;
  background: #fff;
}
.login_out a {
  color: #666666;
  font-size: 14px;
}
.info .pic {
  margin-left: 22px;
  margin-top: 7px;
}
.info .name {
  margin-top: 13px;
  margin-left: 15px;
  color: #ffffff;
  font-size: 14px;
  line-height: 16px;
}
.info .name span {
  display: block;
  color: #919191;
  font-size: 12px;
}
.info .fr {
  margin-top: 29px;
  margin-right: 20px;
}
.ng-error{
  border-color: red !important;
  border-bottom: 1px solid red !important;
}

