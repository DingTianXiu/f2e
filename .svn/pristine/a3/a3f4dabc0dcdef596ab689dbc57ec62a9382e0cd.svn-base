<div ng-controller="loginCtrl">
  <div class="login-header">
    <h1>
        <img src="./public/images/login_logo.png" alt="" />
        <span>WLGF</span>
        <span>账号登录</span>
    </h1>
  </div>
  <form method="post" id="fm1" onsubmit="return sso.loginValidate();" action="" target="ssoLoginFrame" class="login-form">
    <input type="hidden" name="execution" value="" id="fk" />
    <input type="hidden" name="lt" value="" id="lt">
    <input type="hidden" name="_eventId" value="submit" />
    <input type="hidden" name="isajax" value="true" />
    <input type="hidden" name="isframe" value="true" />
    <input type="hidden" name="callback" value="sso.feedBackUrlCallBack" />
    <input type="hidden" name="sysCode" value="wlgf" />
    <div class="login">
      <img src="./public/images/login_show.png" alt="" class="show" />
      <div class="login-right">

        <h2>
            <img src="./public/images/admin_ico.png" alt="" />
            <div class="prompt" id="errorBox">
                <em id="errorStr"></em>
            </div>
        </h2>
        <div class="loginformbox">
          <p class="lf-p lf-user">
            <input type="text" name="loginKey" class="inp-text user" id="loginKey" placeholder="请输入帐号" /><i class="login-ico"></i>
          </p>
          <p class="lf-p lf-password">
            <input type="password" class="inp-text password" name="passwordTemp" id="passwordTemp" placeholder="请输入登录密码" autocomplete="off" /><i class="login-ico"></i>
            <input type="hidden" name="password" id="password">
          </p>
          <!-- <p class="lf-p">
            <input type="text"  id="smsValidateCode" name="smsValidateCode" style="width:170px;" class="inp-reg login-yz" placeholder="请输入验证码" />
            <span class="lf-p-sms"><a href="javascript:;" class="link-light-blue" ng-click="setSmsCode();">获取短信验证码</a></span>
          </p> -->
          <p class="lf-p">

          <input type="text" name="validateCode" class="inp-reg validate" id="validateCode" placeholder="请输入验证码" style="width:170px;margin-right:15px;" value="" />
          <img style="cursor:pointer;" id="capt" class="capt">

                </p>
          <div class="x-btn-loading" id="J-loading" style="display:none;"></div>
          <button type="submit" class="btn btn-s btn-default submit" id="J-submit">登录</button>

        </div>

      </div>
    </div>
  </form>
</div>
