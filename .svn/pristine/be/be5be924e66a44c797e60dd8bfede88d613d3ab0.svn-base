<div ng-controller="createApiCtrl">
  <div class="bq_wrap creat_wrap">
    <h2>新增标签源</h2>
    <form name="createtagform" novalidate>
      <ul class="clear createApi-step">
        <li class="on">
          <h3>1</h3>
          <div>
            <!--<p>Step1</p>-->
            <p>API基础信息</p>
          </div>
        </li>
        <li>...</li>
        <li>
          <h3>2</h3>
          <div>
            <!--<p>Step2</p>-->
            <p>API主要参数</p>
          </div>
        </li>
        <li>...</li>
        <li>
          <h3>3</h3>
          <div>
            <!--<p>Step3</p>-->
            <p>代码示例</p>
          </div>
        </li>
        <li>...</li>
        <li>
          <h3>4</h3>
          <div>
            <!--<p>Step4</p>-->
            <p>错误码解释</p>
          </div>
        </li>
        <li>...</li>
        <li>
          <h3>5</h3>
          <div>
            <!--<p>Step5</p>-->
            <p>完成</p>
          </div>
        </li>
      </ul>
      <div>
        <ul class="editTag creat-tag-ul">
          <li>
            <label>API英文名：</label>

            <validate-input options="validateNameOption" value_name="name" name="initApiData"/>
            <!--<input type="text" name="name" ng-change="validate();" class="cus-select" ng-model="initApiData.name" validate-length="10">
            <span class="color-red">*</span>
            <p class="p-tips">
              <span ng-show="show_name" class="color-red" >{{validate_name_info}}</span>
            </p>-->
          </li>
          <li>
            <label>API中文名：</label>
            <!--<input type="text" name="cname" class="cus-select" ng-model="initApiData.cname" >-->
            <!--<span class="color-red">*</span>-->
            <validate-input options="validateCNameOption" value_name="cname" name="initApiData"/>
          </li>
          <li>
            <label>API版本：</label>
            <!--<input type="text" name="version" class="cus-select" ng-model="initApiData.version">-->
            <!--<span class="color-red">*</span>-->
            <validate-input options="validateVersionOption" value_name="version" name="initApiData"/>
          </li>
          <li id="categoryLi">
            <label>API类目</label>
            <my-ligerui-combo-box ng-model="comboBoxOption" root-text="类目" combo-box-id="createSelectCategory" div-class="category"/>
            <h4 class="quality-tips" id="creatCategory" ng-model = "initApiData.pathName" >{{initApiData.pathName}}</h4>
          </li>
          <li class="tag-instructions">
            <label>API描述：</label>
            <textarea name="comment" class="mx-textarea comment" ng-model="initApiData.comment" required  ng-class="{'ng-error':submited && createtagform.comment.$error.required}"></textarea>
            <p>
              <span ng-show="submited && createtagform.comment.$error.required" class="color-red">表说明为必填项*</span>
              <span ng-show="maxLength == false" class="color-red">表说明最大长度为200</span>
            </p>
            <span class="color-red" ng-show="!(submited && createtagform.comment.$error.required)">*</span>
            <em class="textLength"><em ng-class="{'warning-red':maxLength == false}">{{textLength}}</em>/200</em>
          </li>
        </ul>
      </div>
      
    </form>
  </div>
  <div class="tag-submit">
    <div class="btn-center">
      <a href="javascript:;" class="btn-gray-a a2" ng-click="cancelTag()">取消</a>
      <button class="btn-blue a1" href="javascript:;" ng-click="saveCreateTag()">下一步</button>
    </div>
  </div>
  <div class="mask">
    <div class="mask-bg"></div>
    <div class="imask">
      <p>API设置未完成，你要取消吗？</p>
      <div>
        <button class="btn-gray-a" ui-sref="g.api.apiList">取消</button>
        <button class="btn-blue" ng-click="next()">继续设置</button>
      </div>
    </div>
  </div>
</div>
