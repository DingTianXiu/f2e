            $.each(treeData.children, function (index, item) {
                item.name = $rootScope.escapeHtml(item.name);
                delete item.url;
                delete item.comment;
                if (item.children != null && item.children.length != 0) {
                    item.children = $rootScope.treeDataUtilChilden(item.children);
                }
            });
            return treeData;
        }

