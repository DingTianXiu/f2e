        /**
         * 转换特殊字符
         */
        $rootScope.escapeHtml = function (s,replaceStr) {
            if(!s && replaceStr){
                return replaceStr;
            }
            s = String(s === null ? "" : s);
            return s.replace(/&(?!\w+;)|["'<>\\]/g, function (s) {
                switch (s) {
                    case "&":
                        return "&amp;";
                    case "\\":
                        return "\\\\";
                    case '"':
                        return '&quot;';
                    case "'":
                        return '&#39;';
                    case "<":
                        return "&lt;";
                    case ">":
                        return "&gt;";
                    default:
                        return s;
                }
            });
        }

